<div class="card w-100 mb-3">
  {{#each posting as |post|}}
    <div class="card-body">
        <a href="/post/{{post.id}}" class="fixtag"><h5 class="card-title">{{post.title}}</h5></a>
        <h5 class="card-title">{{post.date}}</h5>
        <h5 class="card-title">By: {{post.user.username}}</h5>
        <p class="card-text">{{post.content}}</p>
      </div>
  {{/each}}
</div>